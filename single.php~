<!-- Main Content -->
<div class="row" role="main">
    <div class="large-12 columns" role="main">

        <?php if ( have_posts() ) : ?>

	    <?php while ( have_posts() ) : the_post(); ?>
	        <?php get_template_part( 'content', 'single' ); ?>
	    <?php endwhile; ?>
	    
        <?php endif; ?>

    </div>
</div>
<!-- End Main Content -->